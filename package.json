{
  "name": "3-way-spades-server",
  "version": "1.0.0",
  "description": "3 player spades server and web app",
  "main": "index.js",
  "scripts": {
    "lint": "standardx --fix src/**/*.ts",
    "test": "jest",
    "test:dev": "jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/johnomalley/3-way-spades-server.git"
  },
  "keywords": [
    "spades",
    "typescript",
    "node"
  ],
  "author": "John O'Malley",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/johnomalley/3-way-spades-server/issues"
  },
  "homepage": "https://github.com/johnomalley/3-way-spades-server#readme",
  "devDependencies": {
    "@types/express": "~4.17.6",
    "@types/jest": "~25.2.1",
    "@types/lodash": "~4.14.150",
    "@types/node": "~12.6.4",
    "@types/randomstring": "~1.1.6",
    "@typescript-eslint/eslint-plugin": "~2.30.0",
    "@typescript-eslint/parser": "~2.30.0",
    "express": "~4.17.1",
    "jest": "~25.5.4",
    "standardx": "~5.0.0",
    "ts-jest": "~25.4.0",
    "typescript": "~3.8.3"
  },
  "eslintConfig": {
    "rules": {
      "no-unused-vars": "off",
      "@typescript-eslint/no-unused-vars": "error"
    },
    "overrides": [
      {
        "files": [
          "**/test/**/*.ts"
        ],
        "globals": {
          "jest": true,
          "describe": true,
          "beforeEach": true,
          "afterEach": true,
          "it": true,
          "expect": true
        }
      }
    ]
  },
  "standardx": {
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint/eslint-plugin"
    ]
  },
  "dependencies": {
    "aws-sdk": "~2.668.0",
    "lodash": "~4.17.15",
    "npm": "~6.14.5",
    "randomstring": "~1.1.5"
  }
}
